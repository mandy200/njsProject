<%- include("./headers/header",{title:title,user : user}) %>
<script src="/javascripts/loadingModel.js"></script>
<script src="/javascripts/tableManager.js"></script>
<div class="container" style="margin-top:30px">
    <div class="row">
        <div class="shops-grid col-xl-9 col-lg-8 order-lg-2">
            <h1><%= title %></h1>
            <div class="row">
                <div class="col-sm-4 card">
                    <form method="post" action="/shops/search">
                        <div class="form-group>">
                            <div  class="form-group">
                                <label for="search">Search :</label>
                                <input class="form-control" id="Search" type="text" name="search" placeholder="Search by Name" required/>
                            </div>
                        </div>
                        <input type="submit" value="Search" class="btn btn-primary">
                    </form>
                </div>
                <div class="col-sm-8 card">
                    <div id="realList">

                        <%-shop%>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<%- include("./headers/footer",{title:"your_title"}) %>